---
interface Props {
  date: Date;
}

const { date } = Astro.props;
const parsedDate = new Date(date);
console.log(parsedDate)
---

<time datetime={parsedDate ? parsedDate.toISOString() : new Date().toISOString()}>
  {
    parsedDate.toLocaleString('en-us', {
      year: 'numeric',
      month: 'short',
      day: 'numeric',
    })
  }
</time>
